<?xml version="1.0"?>
<robot name="duda" xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:macro name="compartment" params="name length width height">
        <link name="${name}">
            <visual>
                <geometry>
                    <box size = "${length} ${width} ${height}">
                </geometry>
            </visual>
        </link>
    </xacro:macro>
    <xacro:compartment name="base_link" length="2.6" width="0.4" height="0.4" />
</robot>
